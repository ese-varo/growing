document.getElementById('new-note-close-btn').click();
document.getElementById('note-container').innerHTML = "<%= j render 'note/note', day_habit: @day %>";
noteIdentifier = document.createElement('div');
noteIdentifier.className = 'had-note';
noteIdentifier.setAttribute('data-bs-toggle', 'tooltip');
noteIdentifier.setAttribute('data-bs-placement', 'bottom');
noteIdentifier.setAttribute('title', 'You have a note');
document.getElementById("day-<%= @day.id %>").appendChild(noteIdentifier);
